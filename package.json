{
  "name": "json-deep-copy",
  "version": "1.1.7",
  "description": "faster deep-copy for json style data",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "standard && jest --coverage",
    "fix": "standard --fix",
    "release": "rm -rf dist && webpack --progress --config webpack.config.babel.js",
    "prepublishOnly": "npm run release",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "benchmark": "node benchmark/deepcopy-vs-lodash-deep-clone.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zxdong262/deep-copy.git"
  },
  "keywords": [
    "deep-copy",
    "json"
  ],
  "author": "ZHAO Xudong <zxdong@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zxdong262/deep-copy/issues"
  },
  "homepage": "https://github.com/zxdong262/deep-copy#readme",
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-typescript": "^7.12.7",
    "@babel/register": "^7.12.1",
    "@tsconfig/recommended": "^1.0.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "coveralls": "^3.1.0",
    "dotenv": "^8.2.0",
    "dotenv-override-true": "^6.2.2",
    "jest": "^26.6.3",
    "standard": "^16.0.3",
    "ts-loader": "^8.0.11",
    "ts-standard": "^9.0.0",
    "typescript": "^4.1.2",
    "webpack": "^5.9.0",
    "webpack-cli": "^4.2.0"
  },
  "jest": {
    "testEnvironment": "node",
    "roots": [
      "./test"
    ],
    "setupFiles": [
      "dotenv-override-true/config"
    ],
    "silent": false
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "*-compiled.js",
      "/dist/"
    ]
  },
  "directories": {
    "test": "test"
  }
}
